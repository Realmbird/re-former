<%= form_with model: @user do |form|%>
<%= label_tag(:user_name, 'Username: ')%>
<%= form.text_field :name, value: "Your Username"%>
<%= label_tag(:user_email, 'Email: ')%>
<%= form.text_field :email, value: "example@gmail.com"%>
<%= label_tag(:user_password, 'Password: ')%>
<%= form.text_field :password, value: "password"%>
<%= form.submit "Create"%>
<% end %>
